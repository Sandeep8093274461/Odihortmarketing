    <script>
        var app = angular.module('myAdminApp', ['ngRoute', 'angular.filter', 'angularUtils.directives.dirPagination', 'angularjs-dropdown-multiselect']);
        app.controller('myAdminCtrl', function ($scope, $http) { });
        app.filter('capitalize', function () {
            return function (input) {
                if (input != null) {
                    input = input.toLowerCase().split(' ');
                    for (var i = 0; i < input.length; i++) {
                        input[i] = input[i].charAt(0).toUpperCase() + input[i].substring(1);
                    }
                    return input.join(' ');
                }
                else {
                    return false;
                }
            }
            // return function (input) {
            //     return (!!input) ? input.charAt(0).toUpperCase() + input.substr(1).toLowerCase() : '';
            // }
        });
        app.config(['$routeProvider', function ($routeProvider) {
            $routeProvider
                .when("/", {
                    templateUrl: "admin/dashboard",
                    controller: 'myAdminDashboardCtrl'
                })
                .when("/availableItemsList", {
                    templateUrl: "admin/availableItemsList",
                    controller: 'myAdminAvailableItemsListCtrl'
                })
                // .when("/", {
                //     templateUrl: "admin/availableItemsList",
                //     controller: 'myAdminAvailableItemsListCtrl'
                // })
                .when("/itemsAvailableDistrictWise", {
                    templateUrl: "admin/itemsAvailableDistrictWise",
                    controller: 'myAdminItemsAvailableDistrictWiseCtrl'
                }).when("/stockInList", {
                    templateUrl: "admin/stockInList",
                    controller: 'myAdminStockInListCtrl'
                }).when("/stockOutList", {
                    templateUrl: "admin/stockOutList",
                    controller: 'myAdminStockOutListCtrl'
                }).when("/stockInOutAvailableDistrictBlockWise", {
                    templateUrl: "admin/stockInOutAvailableDistrictBlockWise",
                    controller: 'myAdminStockInOutAvailableDistrictBlockWiseCtrl'
                }).when("/tradersList", {
                    templateUrl: "admin/tradersList",
                    controller: 'myAdminTradersListCtrl'
                }).when("/farmersCountDistrictWise", {
                    templateUrl: "admin/farmersCountDistrictWise",
                    controller: 'myAdminFarmersCountCtrl'
                }).when("/unavailableItemsList", {
                    templateUrl: "admin/unavailableItemsList",
                    controller: 'myAdminUnavailableItemsListCtrl'
                }).when("/ddhDetails", {
                    templateUrl: "admin/ddhDetails",
                    controller: 'myAdminDDHDetailsCtrl'
                }).when("/areaProductionCW", {
                    templateUrl: "admin/areaProductionCW",
                    controller: 'myAdminAreaProductionCWCtrl'
                }).when("/areaProductionDW", {
                    templateUrl: "admin/areaProductionDW",
                    controller: 'myAdminAreaProductionDWCtrl'
                }).when("/changePassword", {
                    templateUrl: "admin/changePassword",
                    controller: 'myAdminChangePasswordCtrl'
                }).otherwise({
                    redirectTo: "/"
                });
        }]);
        app.directive('loading', ['$http', function ($http) {
            return {
                restrict: 'A',
                template: '<div class="lds-eclipse" style="background: white; z-index: 99999; position: fixed"><div style="margin: 15% 43%"></div></div>',
                // Loading&#8230;
                link: function (scope, elm, attrs) {
                    scope.isLoading = function () {
                        return $http.pendingRequests.length > 0;
                    };
                    scope.$watch(scope.isLoading, function (v) {
                        if (v) {
                            elm.css('display', 'block');
                        } else {
                            elm.css('display', 'none');
                        }
                    });
                }
            };
        }]);
        app.directive('numbersOnly', function () {
            return {
                require: '?ngModel',
                link: function (scope, element, attrs, ngModelCtrl) {
                    if (!ngModelCtrl) {
                        return;
                    }
                    ngModelCtrl.$parsers.push(function (val) {
                        if (angular.isUndefined(val)) {
                            var val = '';
                        }
                        var clean = val.replace(/[^0-9]/g, '');
                        var negativeCheck = clean.split('-');
                        if (!angular.isUndefined(negativeCheck[1])) {
                            negativeCheck[1] = negativeCheck[1].slice(0, negativeCheck[1].length);
                            clean = negativeCheck[0] + '-' + negativeCheck[1];
                            if (negativeCheck[0].length > 0) {
                                clean = negativeCheck[0];
                            }
                        }
                        if (val !== clean) {
                            ngModelCtrl.$setViewValue(clean);
                            ngModelCtrl.$render();
                        }
                        return clean;
                    });
                    element.bind('keypress', function (event) {
                        if (event.keyCode === 32) {
                            event.preventDefault();
                        }
                    });
                }
            };
        });
        app.directive('mobileNoOnly', function () {
            return {
                require: '?ngModel',
                link: function (scope, element, attrs, ngModelCtrl) {
                    if (!ngModelCtrl) {
                        return;
                    }
                    element.on('input change', function () {
                        if (this.value === '0' || this.value === '1' || this.value === '2' || this.value === '3' || this.value === '4' || this.value === '5') {
                            this.value = null;
                        }
                    });
                }
            };
        });
        app.directive('nameOnly', function () {
            return {
                require: '?ngModel',
                link: function (scope, element, attrs, ngModelCtrl) {
                    if (!ngModelCtrl) {
                        return;
                    }
                    ngModelCtrl.$parsers.push(function (val) {
                        if (angular.isUndefined(val)) {
                            var val = '';
                        }
                        var clean = val.replace(/[^a-zA-Z\s]/g, '');
                        if (val !== clean) {
                            ngModelCtrl.$setViewValue(clean);
                            ngModelCtrl.$render();
                        }
                        return clean;
                    });
                }
            };
        });
    </script>
